VertexLitGeneric
{
    "$basetexture" "models\CoDMW2022\Weapons\p90\qx1"

    "$nocull"		"1"
    "$additive" "1"
    "$selfillum" "1" // 启用自发光效果
    "$selfillumtint" "[1 1 1]" // 自发光颜色和亮度，RGB格式
    "$selfillumfresnel" "1" // 启用 Fresnel 效果
    "$translucent" "1"
	"Proxies"
    {
        "AnimatedTexture"               //动画贴图
        {
            "animatedTextureVar" "$basetexture" // 动画贴图变量
            "animatedTextureFrameNumVar" "$frame" // 动画贴图帧数变量
            "animatedTextureFrameRate" 24.00 // 动画贴图帧率
        }
    }
}